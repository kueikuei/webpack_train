{
  "env": {
    "browser": true
  },
  "extends": "eslint-config-airbnb-es5",
  "rules": {
    "indent": ["error", 4],
    "no-console": 1,
    "max-len": [1, 120],
    "import/prefer-default-export": 1,
    "no-param-reassign": 0,
    "prefer-rest-params": 0,
    "no-underscore-dangle": 0,
    "no-shadow": 0,
    "func-names": 0
  }
}


